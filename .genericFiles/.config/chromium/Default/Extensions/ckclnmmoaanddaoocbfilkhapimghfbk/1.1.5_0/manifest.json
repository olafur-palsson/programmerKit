{
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/icon_19.png",
      "default_title": "Material Design YouTube Dark Skin Theme : Black and Red"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "material_design_theme.min.js" ],
      "matches": [ "*://www.youtube.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "YouTube dark skin theme : designed specially for night to avoid flash effect",
   "icons": {
      "19": "images/icon_19.png",
      "48": "images/icon_48.png",
      "96": "images/icon_96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Material Design Dark Skin Theme for YouTubeâ„¢",
   "permissions": [ "storage" ],
   "short_name": "Material Black & Red",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.5",
   "web_accessible_resources": [ "images/*", "material_design_theme.min.css" ]
}
