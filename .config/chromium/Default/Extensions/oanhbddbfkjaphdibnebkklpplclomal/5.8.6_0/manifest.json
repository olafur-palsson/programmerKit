{
   "background": {
      "persistent": false,
      "scripts": [ "js/libs/jquery-1.11.1.js", "js/libs/jquery.xmlrpc.js", "js/ga.js", "js/OpenSubtitles.js", "js/Amara.js", "js/background.js" ]
   },
   "content_scripts": [ {
      "css": [ "css/style.css" ],
