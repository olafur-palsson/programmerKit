{
   "author": [ "Michael Walz", "Brandon Richardson" ],
   "background": {
      "persistent": false,
      "scripts": [ "/backend/backend.js" ]
   },
   "browser_action": {
      "default_icon": "resources/icon.png",
      "default_popup": "popup/popup.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "Ctrl+Shift+F"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "/content/highlighter.css" ],
      "js": [ "/lib/html-entity-handler/entityhandler.js", "/content/content.js", "/content/highlighter.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "A find-in-page extension for Google Chrome with support for regular expressions.",
   "icons": {
      "128": "resources/icon.png",
      "16": "resources/icon.png",
      "48": "resources/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "find+ | Regex Find-in-Page Tool",
   "offline_enabled": true,
   "omnibox": {
      "keyword": "find"
   },
   "permissions": [ "tabs", "activeTab", "background", "storage", "\u003Call_urls>" ],
   "short_name": "find+",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.3"
}
